{
  "name": "angular-json-class-converter",
  "version": "3.0.1",
  "description": "Angular module to serialize and deserialize JS/TS objects to JSON. ",
  "keywords": [
    "Angular",
    "Library",
    "JSON",
    "serialize",
    "deserialize",
    "GSON",
    "class"
  ],
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "https://github.com/doron-mil/angular-json-class-converter"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-test": "ng build testApp --prod --outputPath='dist_test'",
    "test": "ng test",
    "run server": "node ./testApp/testServer/server.js",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build_lib": "ng build angular-json-class-converter",
    "copy-license": "cp ./LICENSE ./dist/angular-json-class-converter",
    "copy-readme": "cp ./README.md ./dist/angular-json-class-converter",
    "copy-files": "npm run copy-license && npm run copy-readme",
    "npm_pack": "cd dist/angular-json-class-converter && npm pack",
    "package": "npm run build_lib && npm run copy-files && npm run npm_pack"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "core-js": "^2.5.4",
    "json-class-converter": "^1.0.1",
    "moment": "^2.24.0",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular-devkit/build-ng-packagr": "~0.13.0",
    "@angular/cli": "~7.3.8",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "express": "^4.17.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.2.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.34.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  }
}
